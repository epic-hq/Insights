{
	"$schema": "https://unpkg.com/knip@5/schema.json",
	"entry": ["scripts/*.{ts,js}", "app/server/*.ts", "app/library/icon/Icon.tsx"],
	"remix": true,
	"lefthook": true,
	"project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"],
	"ignore": [
		"app/library/icon/icons/types.ts",
		"react-router.config.ts",
		".storybook/**",
		"app/components/**",
		"app/data/**",
		"app/extraction/**",
		"app/features/**",
		"app/hooks/use-supabase-upload.ts",
		"app/layouts/**",
		"app/lib/cms/**",
		"app/lib/database/junction-*.ts*",
		"app/lib/hooks/useJunctionTables.ts",
		"app/lib/observedFetch.ts",
		"app/lib/supabase/middleware.ts",
		"app/library/**",
		"app/storybook/**",
		"app/hooks/use-project-path.ts",
		"app/utils/env.ts",
		"app/utils/evidence.server.ts",
		"app/utils/logger.ts",
		"app/utils/signedRead.server.ts",
		"app/utils/transcript-navigation.ts",
		"app/utils/verify-jwt.server.ts",
		"app/mastra/mcp-server.ts",
		"app/mastra/tools/signup-completion-guard.ts",
		"baml_client/**",
		"public/worklets/**",
		"src/trigger/**",
		"supabase/functions/**",
		"test-*.js",
		"trigger.config.ts",
		"trigger/transcode.ts",
		"worker/src/index.ts",
		"index.js",
		"middleware.ts"
	],
	"ignoreDependencies": [
		"@babel/preset-typescript",
		"tailwindcss",
		"tw-animate-css"
	],
	"ignoreBinaries": ["supabase"]
}
