  "scripts": {
    "baml-generate": "baml-cli generate && node scripts/patch-baml.mjs",
    "build": "export NODE_OPTIONS=--max-old-space-size=4096; pnpm run baml-generate && react-router build",
    "build-storybook": "storybook build",
    "check": "biome check .",
    "check:fix": "biome check --write --unsafe .",
    "check:fix:chunks": "biome check app/components --write --unsafe && biome check app/features --write --unsafe && biome check app/routes --write --unsafe && biome check scripts --write --unsafe",
    "check:unused": "knip --max-issues 100 --no-exit-code",
    "check:unused:fix": "knip --fix",
    "clean": "git clean -fdX --exclude=\"!.env\"",
    "db:types": "supabase gen types --project-id rbginqvgkonnoktrttqv --schema public,accounts,billing typescript > supabase/types.ts",
    "dev": "concurrently -n \"Vite,Mastra\" --c green,cyan \"pnpm run dev:vite\" \"pnpm run dev:mastra\"",
    "dev:agent": "dotenvx run -- tsx agents/livekit/agent.ts dev",
    "dev:mastra": "dotenvx run -- tsx scripts/dev-mastra.ts",
    "dev:vite": "mkdir -p build/client && PORT=4280 dotenvx run -- react-router dev",
    "dev:with-agent": "concurrently -n \"Vite,Mastra,Agent\" --c green,cyan,magenta \"pnpm run dev:vite\" \"pnpm run dev:mastra\" \"pnpm run dev:agent\"",
    "email": "email dev",
    "execute": "tsx",
    "migrate": "supabase migrations up",
    "postmigrate": "pnpm run db:types",
    "prebuild": "pnpm run baml-generate",
    "script": "tsx scripts/setup.ts",
    "start": "dotenvx run -- node ./build/server/index.js",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run --browser.headless",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:browser": "vitest run --config vitest.browser.config.ts --browser.headless",
    "benchmark:howto:smoke": "tsx scripts/howto-benchmark-smoke.ts",
    "benchmark:howto:update-snapshot": "tsx scripts/howto-benchmark-smoke.ts --update-snapshot",
    "test:agents": "vitest run app/routes/api.chat.project-status.test.ts app/mastra/tools/__tests__/fetch-project-status-context.test.ts app/mastra/tools/__tests__/fetch-top-themes-with-people.test.ts && pnpm benchmark:howto:smoke",
    "test:agents:smoke": "tsx scripts/agent-smoke-prompts.ts",
    "test:all": "vitest run --browser.headless && vitest run --config vitest.integration.config.ts",
    "test:cov": "vitest run --coverage --browser.headless",
    "test:integration": "dotenvx run -- vitest run --config vitest.integration.config.ts",
    "test:integration:ui": "dotenvx run -- vitest --config vitest.integration.config.ts",
    "test:ui": "vitest --config vitest.unit.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "typecheck": "react-router typegen && tsc -p tsconfig.typecheck.json",
    "typegen": "react-router typegen",
    "dev:desktop": "pnpm --filter @insights/desktop start",
    "build:desktop": "pnpm --filter @insights/desktop make",
    "package:desktop": "pnpm --filter @insights/desktop package",
    "validate": "pnpm run check && pnpm run typecheck && pnpm run test && pnpm run check:unused"
  },
  "sideEffects": false,
  "type": "module",
  "version": "0.0.1"
