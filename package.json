	"name": "@forge42/base-stack",
	"version": "0.0.1",
	"author": "forge-42",
	"private": true,
	"sideEffects": false,
	"license": "MIT",
	"type": "module",
	"scripts": {
		"execute": "tsx",
		"clean": "git clean -fdX --exclude=\"!.env\"",
		"script": "tsx scripts/setup.ts",
		"build": "react-router build",
		"dev:vite": "PORT=4280 dotenvx run -- react-router dev",
		"dev": "concurrently -n \"Vite,Mastra\" --c green,cyan \"pnpm run dev:vite\" \"pnpm run dev:mastra\"",
		"dev:mastra": "dotenvx run -- npx mastra dev --dir app/mastra",
		"start": "dotenvx run -- node ./build/server/index.js",
		"test": "vitest run --browser.headless",
		"test:ui": "vitest",
		"test:cov": "vitest run --coverage --browser.headless",
		"test:integration": "vitest run --config vitest.integration.config.ts",
		"test:integration:ui": "vitest --config vitest.integration.config.ts",
		"test:all": "vitest run --browser.headless && vitest run --config vitest.integration.config.ts",
		"typecheck": "react-router typegen && tsc -p tsconfig.typecheck.json",
		"validate": "pnpm run check && pnpm run typecheck && pnpm run test && pnpm run check:unused",
		"check": "biome check .",
		"check:fix": "biome check --write --unsafe .",
		"check:unused": "knip --max-issues 100 --no-exit-code",
		"check:unused:fix": "knip --fix",
		"typegen": "react-router typegen",
		"db:types": "supabase gen types typescript --linked > supabase/types.ts",
		"migrate": "supabase migrations up",
		"postmigrate": "pnpm run db:types",
		"baml-generate": "baml-cli generate"
	},
