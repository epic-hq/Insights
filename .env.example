# Add your env variables here

# The environment of the app. This is different from NODE_ENV, and will be used for deployments.
# Default: development
#APP_ENV="staging"

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Integration test Supabase (dedicated non-production instance)
TEST_SUPABASE_URL=your_test_supabase_url
TEST_SUPABASE_ANON_KEY=your_test_supabase_anon_key
TEST_SUPABASE_SERVICE_ROLE_KEY=your_test_supabase_service_role_key
# Optional: explicit safety check
TEST_SUPABASE_PROJECT_REF=your_test_project_ref

# OpenAI
OPENAI_API_KEY=your_openai_api_key

# Mastra
MASTRA_URL=http://localhost:4111
MASTRA_PORT=4111

# LiveKit
LIVEKIT_SFU_URL=
LIVEKIT_API_KEY=
LIVEKIT_API_SECRET=
LIVEKIT_TTL_SECONDS=300

# PostHog (client analytics)
POSTHOG_KEY=your_posthog_project_api_key
POSTHOG_HOST=https://us.i.posthog.com

# Polar (billing)
POLAR_ACCESS_TOKEN=
POLAR_SUCCESS_URL=http://getupsight.com/success?checkout_id={CHECKOUT_ID}

# Email (Engage.so)
ENGAGE_API_KEY=
ENGAGE_API_SECRET=
DEFAULT_FROM_EMAIL=
DEFAULT_FROM_EMAIL_NAME=

# Legacy Resend Email (deprecated)
# RESEND_API_KEY=

# Cloudflare R2
R2_ACCOUNT_ID=
R2_ACCESS_KEY_ID=
R2_SECRET_ACCESS_KEY=
R2_BUCKET_NAME=
R2_PUBLIC_BASE_URL=
